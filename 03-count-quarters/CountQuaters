public class Main {
 public static void main(String[] args) {
   System.out.println(quater(1150));
    }
    public static int quater(int num) {
    String nums = Integer.toString(num);
    String var;
    if (nums.length() < 1) {
            return 0; } 
            else {
            var = nums.substring(nums.length() - 2);
            int num1 = Integer.parseInt(var) / 25;
            return num1;
        }
    }
}
